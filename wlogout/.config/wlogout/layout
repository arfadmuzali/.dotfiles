{
    "label" : "lock",
    "action" : "swaylock",
    "keybind" : "l"
}
{
    "label" : "hibernate",
    "action" : "shutdown -c && notify-send 'Shutdown canceled :('",
    "keybind" : "c"
}
{
    "label" : "logout",
    "action" : "loginctl terminate-user $USER",
    "keybind" : "e"
}
{
    "label" : "shutdown",
    "action" : "systemctl poweroff",
    "keybind" : "s"
}
{
    "label" : "suspend",
    "action" : "shutdown +60 && notify-send 'Shutdown in 60 min'",
    "keybind" : "u"
}
{
    "label" : "reboot",
    "action" : "systemctl reboot",
    "keybind" : "r"
}
